{
    "version": 2,
    "builds": [
      {
        "src": "intern-backend folder/backend/mains/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "inter-frontend folder/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/intern-backend folder/backend/mains/main.py"
      },
      {
        "src": "/css/(.*)",
        "dest": "/inter-frontend folder/css_folder"
      },
      {
        "src": "/js/(.*)",
        "dest": "/inter-frontend folder/javascript_folder"
      },
      {
        "src": "/images/(.*)",
        "dest": "/inter-frontend folder/image"
      },
      {
        "src": "/(.*)",
        "dest": "/inter-frontend folder/html_folder/index.html"
      }
    ]
}
  